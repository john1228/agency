<table class="table table-striped table-list" id="datatable-editable">
  <thead>
  <tr>
    <th>门店</th>
    <th>标题</th>
    <th>第一栏</th>
    <th>第二栏</th>
    <th>第三栏</th>
    <th>操作</th>
  </tr>
  </thead>
  <tbody>
  <% @banners.each { |banner| %>
      <tr>
        <td>
          <%= banner.service.profile.name %>
        </td>
        <td>
          <%= banner.title %>
        </td>
        <td>
          <%= image_tag(banner.pos_1.first.url + '-avatar') %>
        </td>
        <td>
          <%= image_tag(banner.pos_2.first.url + '-avatar') %>
        </td>
        <td>
          <%= image_tag(banner.pos_3.first.url + '-avatar') %>
        </td>
        <td class="sign-in">
          <%= link_to('编辑', edit_s_banner_path(banner)) %>
          |<%= link_to('删除', banner, method: :delete, data: {confirm: '您确定要删除吗？'}) %>
        </td>
      </tr>
  <% } %>
  </tbody>
</table>